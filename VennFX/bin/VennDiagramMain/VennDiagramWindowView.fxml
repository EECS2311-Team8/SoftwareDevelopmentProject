         </children>
      </Pane>
      <StackPane fx:id="stackPane" layoutX="58.0" layoutY="155.0" prefHeight="494.0" prefWidth="778.0">
         <children>
            <Circle fx:id="circle1" fill="#1f93ff00" radius="245.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="3.0" StackPane.alignment="BOTTOM_LEFT" />
